{
  "name": "datematrix",
  "version": "1.0.0",
  "description": "generated date matrix",
  "keywords": [
    "datematrix",
    "date-matrix",
    "lunar calendar",
    "china lunar calendar",
    "农历"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "prepublish": "rimraf dist && unbuild"
  },
  "devDependencies": {
    "rimraf": "3.0.2",
    "unbuild": "0.7.4",
    "vite": "2.9.9"
  },
  "author": "zhu-hong",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/zhu-hong/use-date-matrix"
  },
  "homepage": "https://github.com/zhu-hong/use-date-matrix"
}
